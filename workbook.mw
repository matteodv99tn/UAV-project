<Group labelreference="L1030" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">export_function := proc(
		export_name::string,
		function,
		x := false,
		u := false,
		p := false,
		jac_x := false,
		jac_u := false) 
	
	global data, generated_codes, export_code;
	local code, code_file;

	if (export_code = false) then return; end if;
	
	code := c_code_initialize(export_name):
	code_file := cat(&quot;/tmp/&quot;, export_name, &quot;.json&quot;):
	generated_codes := [op(generated_codes), code_file]:
	c_code_assign_function(code, function):
	
	if (x &lt;&gt; false) then c_code_assign_state(code, x) end if:
	if (u &lt;&gt; false) then c_code_assign_input(code, u) end if:
	if (p &lt;&gt; false) then c_code_assign_parameters(code, p, subs(data, p)) end if:
	if (jac_x &lt;&gt; false) then c_code_set_jacobian_x(code, jac_x) end if:
	if (jac_u &lt;&gt; false) then c_code_set_jacobian_u(code, jac_u) end if:
	
	if allow_code_generation then
	if multithreaded then
		thread_ids := [op(thread_ids), Threads:-Create(c_code_to_json(code, code_file))]:
	else 
		c_code_to_json(code, code_file):
	end if:
	end if:

end proc:</Text-field>
</Input>
</Group>
